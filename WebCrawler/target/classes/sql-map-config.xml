<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- Alias 설정 -->
	<typeAliases>
		<typeAlias type="com.crawler.biz.info.InfoVO" alias="info"></typeAlias>
		<typeAlias type="com.crawler.biz.data.DataVO" alias="data"></typeAlias>
		<typeAlias type="com.crawler.biz.user.UserVO" alias="user"></typeAlias>
		<typeAlias type="com.crawler.biz.board.BoardVO" alias="board"></typeAlias>
		<typeAlias type="com.crawler.biz.board.BoardHaveInfoVO" alias="bhi"></typeAlias>
		<typeAlias type="com.crawler.biz.room.RoomVO" alias="room"></typeAlias>
	</typeAliases>
	
	<!-- Sql Mapper 설정 -->
	<mappers>
		<mapper resource="mappings/info-mapping.xml" />
		<mapper resource="mappings/data-mapping.xml" />
		<mapper resource="mappings/users-mapping.xml" />
		<mapper resource="mappings/board-mapping.xml" />
		<mapper resource="mappings/room-mapping.xml"/>
	</mappers>
</configuration>
