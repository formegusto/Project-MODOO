<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- Alias 설정 -->
	<typeAliases>
		<typeAlias type="com.modoo.wrk.users.UsersVO" alias="users"></typeAlias>
		<typeAlias type="com.modoo.wrk.info.InfoVO" alias="info"></typeAlias>
		<typeAlias type="com.modoo.wrk.data.DataVO" alias="data"></typeAlias>
		<typeAlias type="com.modoo.wrk.tm.TmVO" alias="tm"></typeAlias>
		<typeAlias type="com.modoo.wrk.tm.THIVO" alias="thi"></typeAlias>
		<typeAlias type="com.modoo.wrk.visual.VisualVO" alias="visual"></typeAlias>
		<typeAlias type="com.modoo.wrk.visual.VHIVO" alias="vhi"></typeAlias>
		<typeAlias type="com.modoo.wrk.frame.FrameVO" alias="frame"></typeAlias>
		<typeAlias type="com.modoo.wrk.frame.FHIVO" alias="fhi"></typeAlias>
		<typeAlias type="com.modoo.wrk.board.BoardVO" alias="board"></typeAlias>
		<typeAlias type="com.modoo.wrk.board.BHDVO" alias="bhd"></typeAlias>
		<typeAlias type="com.modoo.wrk.comment.CommentVO" alias="comment"></typeAlias>
		<typeAlias type="com.modoo.wrk.room.RoomVO" alias="room"></typeAlias>
		<typeAlias type="com.modoo.wrk.room.RHDVO" alias="rhd"></typeAlias>
	</typeAliases>
	
	<!-- Sql Mapper 설정 -->
	<mappers>
		<mapper resource="mappings/users-mapping.xml" />
		<mapper resource="mappings/info-mapping.xml" />
		<mapper resource="mappings/data-mapping.xml" />
		<mapper resource="mappings/tm-mapping.xml" />
		<mapper resource="mappings/visual-mapping.xml" />
		<mapper resource="mappings/frame-mapping.xml" />
		<mapper resource="mappings/board-mapping.xml" />
		<mapper resource="mappings/comment-mapping.xml" />
		<mapper resource="mappings/room-mapping.xml" />
	</mappers>
</configuration>
